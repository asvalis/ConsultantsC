<!-- Carousel
================================================== -->
<div id="myCarousel" class="carousel slide" data-interval="3250">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="item active">
            <%= image_tag('g1.png') %>
            <div class="container">
                <div class="carousel-caption">
                    <h1>single-origin coffee pop-up tofu</h1>
                    <p>Shoreditch direct trade Echo Park kitsch stumptown gluten-free.
                        <a title="Bootstrap 3" href="http://getbootstrap.com" class="">Bootstrap 3 RC 1 is now available!</a></p>
                    <p><a class="btn btn-large btn-primary" href="#">Sign up today</a>
                    </p></div>
            </div>
        </div>
        <div class="item">
            <div id="fagoot"><%= image_tag(@picture.image) %></div>
            <div class="container">
                <div class="carousel-caption">
                    <h1>single-origin coffee pop-up tofu</h1>
                    <p>Shoreditch direct trade Echo Park kitsch stumptown gluten-free.</p>
                    <p><a class="btn btn-large btn-primary" href="#">Learn more</a></p>
                </div>
            </div>
        </div>
        <div class="item">
            <img class="myCarPic" src="../assets/g3.png">
            <div class="container">
                <div class="carousel-caption">
                    <h1>single-origin coffee pop-up tofu</h1>
                    <p>Shoreditch direct trade Echo Park kitsch stumptown gluten-free.</p>
                    <p><a class="btn btn-large btn-primary" href="#">Browse gallery</a></p>
                </div>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
</div>




<!-- /.carousel -->



